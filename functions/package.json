{
  "name": "functions",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "watch": "tsc --watch",
    "server": "nodemon dist/functions/src/index.js",
    "serve": "npm-run-all --parallel watch server",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test-init": "./node_modules/jasmine/bin/jasmine.js init",
    "test": "nodemon --ext ts --exec 'jasmine-ts \"spec/**/*.spec.ts\"'",
    "compile": "webpack --config webpack.server.config.js --progress --colors"
  },
