{
  "name": "functions",
  "scripts": {
    "setup": "npm install && npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "server": "nodemon dist/functions/src/index.js",
    "serve": "npm-run-all --parallel watch server",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "dist/functions/src/index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "expressjs": "^1.0.1",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.1.0",
    "jsdom": "^13.0.0",
    "moment": "^2.23.0",
    "newsapi": "^2.4.0",
    "ngx-moment": "^3.3.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "sharp": "^0.21.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "typescript": "~2.8.3",
    "@types/node": "^10.12.12"
  },
  "private": true
}
