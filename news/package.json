{
  "name": "news",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm install",
    "ng": "ng",
    "start": "ng serve --poll 2000",
    "build": "npm run build:browser && npm run build:ssr",
    "build:browser": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run news:serve-ssr",
    "serve:ssr": "node dist/news/server/main.js",
    "build:ssr": "ng build && ng run news:server",
    "prerender": "ng run news:prerender",
    "postbuild": "node postbuild.js",
    "explore": "ng build --source-map && source-map-explorer dist/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.9",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.3.9",
    "@angular/compiler": "~13.3.9",
    "@angular/core": "~13.3.9",
    "@angular/forms": "~13.3.9",
    "@angular/localize": "~13.3.9",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.3.9",
    "@angular/platform-browser-dynamic": "~13.3.9",
    "@angular/platform-server": "~13.3.9",
    "@angular/router": "~13.3.9",
    "@angular/service-worker": "~13.3.9",
    "@nguniversal/express-engine": "^13.1.1",
    "@ngx-pwa/local-storage": "^12.1.0",
    "angular-google-tag-manager": "^1.4.2",
    "autoprefixer": "^10.3.4",
    "dayjs": "^1.10.6",
    "express": "^4.15.2",
    "firebase": "^8.7.0",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.12.0",
    "ismobilejs": "^1.1.1",
    "ng-lazyload-image": "^9.1.0",
    "ngx-clipboard": "^14.0.1",
    "postcss": "^8.3.6",
    "rxjs": "~6.6.0",
    "smoothscroll-polyfill": "^0.4.4",
    "speakingurl": "^14.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.3.9",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/firebase": "^3.2.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/speakingurl": "^13.0.3",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^2.2.15",
    "typescript": "~4.6.4"
  }
}
