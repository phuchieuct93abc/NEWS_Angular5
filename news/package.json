{
  "name": "news",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm install",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "npm run build:browser && npm run build:ssr",
    "build:browser": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run news:serve-ssr",
    "serve:ssr": "node dist/news/server/main.js",
    "build:ssr": "ng build && ng run news:server",
    "prerender": "ng run news:prerender",
    "postbuild": "node postbuild.js",
    "postinstall": "ngcc",
    "explore": "ng build --source-map && source-map-explorer dist/**/*.js",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
